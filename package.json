{
  "name": "hello-gatsby",
  "description": "My starter kit for Gatsby projects based on gatsby-starter-typescript-plus",
  "version": "1.0.0",
  "private": false,
  "author": "Chris Tham <chris.tham@hellotham.com>",
  "keywords": [
    "gatsby",
    "material-ui",
    "typescript",
    "storybook",
    "jest",
    "cypress"
  ],
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-env.js"
  },
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "lc": "npm run type-check --scripts-prepend-node-path && npm run lint --scripts-prepend-node-path",
    "test": "jest",
    "type-check": "tsc",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@mdi/js": "^5.0.45",
    "@mdi/react": "^1.3.0",
    "@reduxjs/toolkit": "^1.3.2",
    "@types/mathjs": "^6.0.4",
    "babel-jest": "^25.2.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "gatsby": "^2.20.8",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-react-redux": "^1.1.0-0",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-theme-material-ui": "^1.0.8",
    "gatsby-transformer-sharp": "^2.4.3",
    "mathjs": "^6.6.2",
    "modern-normalize": "^0.6.0",
    "normalize.css": "^8.0.1",
    "polished": "^3.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "babel-plugin-remove-graphql-queries": "^2.8.1",
    "babel-preset-gatsby": "^0.3.1",
    "babel-preset-react-app": "^9.1.2",
    "cypress": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-blvd": "^1.0.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "prettier": "^2.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.10.11",
    "storybook-addon-material-ui": "^0.9.0-alpha.21",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2"
  }
}
